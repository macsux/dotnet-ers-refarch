{
  "spring": {
    "application": {
      "name": "gateway"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Ocelot": "Debug"
    }
  },
  "AllowedHosts": "*",
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true,
      "ValidateCertificates": false
    },
    "instance": {
      "port": 5002,
      "hostName": "localhost",
      "instanceId": "gateway1"
    }
  }
}
